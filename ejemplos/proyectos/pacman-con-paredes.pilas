    "actores": [
      {
        "nombre": "pacman",
        "codigo": "// @ts-ignore\nclass pacman extends Actor {\n\n    iniciar() {\n        this.animacion = \"caminar\";\n    }\n\n    actualizar() {\n\n        this.pilas.camara.seguir_al_actor(this);\n\n        if (this.control.izquierda) {\n            this.x -= 5;\n            this.rotacion = 180;\n            this.y = this.pilas.ajustar(this.y, 64);\n        }\n\n        if (this.control.derecha) {\n            this.x += 5;\n            this.rotacion = 0;\n            this.y = this.pilas.ajustar(this.y, 64);\n        }\n\n        if (this.control.arriba) {\n            this.y += 5;\n            this.rotacion = 90;\n            this.x = this.pilas.ajustar(this.x, 64);\n        }\n\n        if (this.control.abajo) {\n            this.y -= 5;\n            this.rotacion = -90;\n            this.x = this.pilas.ajustar(this.x, 64);\n        }\n\n        this.evitar_choque_con_paredes();\n    }\n\n    evitar_choque_con_paredes() {\n        if (this.rotacion == 0) {\n            // moviendo hacia la derecha\n\n            if (this.pilas.existe_actor_con_etiqueta_en(\"bloque\", this.x + 32, this.y)) {\n                this.x = this.pilas.ajustar(this.x, 64);\n            }\n        }\n\n        if (this.rotacion == 180) {\n            // moviendo hacia la izquierda\n\n            if (this.pilas.existe_actor_con_etiqueta_en(\"bloque\", this.x - 32, this.y)) {\n                this.x = this.pilas.ajustar(this.x, 64);\n            }\n        }\n\n        if (this.rotacion == 90) {\n            // moviendo hacia arriba\n\n            if (this.pilas.existe_actor_con_etiqueta_en(\"bloque\", this.x, this.y + 32)) {\n                this.y = this.pilas.ajustar(this.y, 64);\n            }\n        }\n\n        if (this.rotacion == -90) {\n            // moviendo hacia abajo\n\n            if (this.pilas.existe_actor_con_etiqueta_en(\"bloque\", this.x, this.y - 32)) {\n                this.y = this.pilas.ajustar(this.y, 64);\n            }\n        }\n    }\n\n    cuando_colisiona(actor: Actor) {\n        actor.eliminar();\n        this.reproducir_sonido(\"comer\");\n    }\n}\n"
      },
