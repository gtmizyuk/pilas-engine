        "codigo": "// @ts-ignore\nclass actor_mono extends Actor {\n\n    iniciar() {\n    }\n\n    actualizar() {\n        this.pilas.observar(\"rotacion\", this.rotacion);\n    }\n}\n"
      },
      {
        "nombre": "pizarra",
        "codigo": "// @ts-ignore\nclass pizarra extends PizarraBase {\n    posicion: number = 0;\n\n    iniciar() {\n        super.iniciar();\n        this.imagen = \"imagenes:basicos/invisible\";\n        this.dibujar_ejes();\n    }\n\n    dibujar_ejes() {\n        this.limpiar();\n\n        // Dibuja los dos ejes\n        this.dibujar_linea(0, 0, 0, 200, \"blanco\", 2);\n        this.dibujar_linea(0, 0, 200, 0, \"blanco\", 2);\n\n        // Dibuja la punta de la flecha del eje vertical.\n        this.dibujar_linea(-5, 195, 0, 200, \"blanco\", 2);\n        this.dibujar_linea(0, 200, 5, 195, \"blanco\", 2);\n\n        // Dibuja la punta de la flecha del eje horizontal.\n        this.dibujar_linea(195, -5, 200, 0, \"blanco\", 2);\n        this.dibujar_linea(200, 0, 195, 5, \"blanco\", 2);\n\n        // Dibuja la linea de los 90 grados\n        this.dibujar_linea(0, 90, 200, 90, \"blanco\", 0.5);\n    }\n\n    actualizar() {\n        let mono = this.pilas.obtener_actor_por_nombre(\"actor_mono\");\n\n        this.posicion++;\n\n        if (this.posicion < 200) {\n            this.dibujar_circulo(this.posicion, mono.rotacion, 1, \"blanco\");\n        }\n\n    }\n\n    reiniciar() {\n        this.limpiar();\n        this.dibujar_ejes();\n        this.posicion = 0;\n    }\n}\n"
      },
