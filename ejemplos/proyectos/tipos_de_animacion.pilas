        "codigo": "// @ts-ignore\nclass boton_suave extends ActorTextoBase {\n\n    cuando_hace_click() {\n        let pizarra: any = this.pilas.obtener_actor_por_nombre(\"pizarra\");\n        let mono = this.pilas.obtener_actor_por_nombre(\"actor_mono\");\n\n        if (this.transparencia > 0) {\n            return;\n        }\n\n        pizarra.reiniciar();\n\n        mono.animar(Tipo.suave).\n            rotar(90, 3.3).\n            rotar_hasta(0, 0.1);\n\n        this.enviar_mensaje_global(\"desactivar_botones\");\n    }\n\n\n    cuando_llega_el_mensaje_desactivar_botones() {\n        this.transparencia = 50;\n        this.pilas.luego(4, () => {\n            this.transparencia = 0;\n        });\n    }\n}\n"
