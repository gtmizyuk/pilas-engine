  "codigos": {
    "escenas": [
      {
        "nombre": "escena2",
        "codigo": "class escena2 extends Escena {\n  actor_a_seguir: Actor;\n  nave: Actor;\n  enemigo: Actor;\n\n  iniciar() {\n    this.nave = this.pilas.obtener_actor_por_nombre(\"nave\");\n    this.enemigo = this.pilas.obtener_actor_por_nombre(\"enemigo\");\n\n    this.actor_a_seguir = this.nave;\n\n\n    // Secuencia de conversaci√≥n\n\n    this.pilas.luego(0, () => {\n      this.actor_a_seguir = this.enemigo;\n      this.actor_a_seguir.decir(\"¬øAcaso sabes qu√© hora es?\");\n    });\n\n\n    this.pilas.luego(4, () => {\n      this.actor_a_seguir = this.nave;\n      this.actor_a_seguir.decir(\"no, ¬øqu√© hora es?\");\n    });\n\n    this.pilas.luego(8, () => {\n      this.actor_a_seguir = this.enemigo;\n      this.actor_a_seguir.decir(\"¬°La hora de tu fin!\");\n    });\n\n    this.pilas.luego(12, () => {\n      this.actor_a_seguir = this.nave;\n      this.actor_a_seguir.decir(\"jaja, ya lo veremos...\");\n    });\n\n    this.pilas.luego(14, () => {\n      this.pilas.clonar(\"texto\");\n    });\n\n  }\n\n\n  actualizar() {\n\n    this.observar(\"actor_a_seguir\", this.actor_a_seguir)\n\n\n    if (this.actor_a_seguir) {\n      this.pilas.camara.seguir_al_actor(this.actor_a_seguir);\n    }\n  }\n}"
      }
    ],
    "actores": [
      {
        "nombre": "nave",
        "codigo": "// @ts-ignore\nclass nave extends Actor {\n\n  velocidad = 5;\n  cuadros_desde_el_ultimo_disparo;\n\n  iniciar() {\n    this.animacion = \"nave_en_reposo\";\n    this.cuadros_desde_el_ultimo_disparo = 0;\n  }\n\n  actualizar() {\n    this.cuadros_desde_el_ultimo_disparo += 1;\n\n    if (this.pilas.control.izquierda) {\n      this.rotacion += this.velocidad;\n      this.animacion = \"nave_girando_a_la_izquierda\";\n    }\n\n    if (this.pilas.control.derecha) {\n      this.rotacion -= this.velocidad;\n      this.animacion = \"nave_girando_a_la_derecha\";\n    }\n\n    if (this.pilas.control.espacio && this.cuadros_desde_el_ultimo_disparo > 5) {\n      this.disparar();\n    }\n\n    if (this.pilas.control.arriba) {\n      this.avanzar(this.rotacion, this.velocidad);\n      this.animacion = \"nave_avanzando\";\n    } else {\n      if (!this.pilas.control.izquierda && !this.pilas.control.derecha) {\n        this.animacion = \"nave_en_reposo\";\n      }\n    }\n  }\n\n  disparar() {\n    let laser = this.pilas.actores.laser();\n    laser.x = this.x;\n    laser.y = this.y;\n    laser.rotacion = this.rotacion;\n    laser.z = this.z + 1;\n    this.cuadros_desde_el_ultimo_disparo = 0;\n  }\n}\n"
      },
      {
        "nombre": "enemigo",
        "codigo": "// @ts-ignore\nclass enemigo extends Actor {\n\n  iniciar() {\n\n    this.pilas.luego(17, () => {\n      // a los 17 segundos termina el dialogo.\n      this.animar(Tipo.lineal, -1).\n        mover_x_hasta(-150).\n        mover_x_hasta(150);\n    });\n\n  }\n\n  actualizar() { }\n\n  cuando_colisiona(actor: Actor) {\n    if (actor.tiene_etiqueta(\"laser\")) {\n      this.eliminar();\n      this.pilas.clonar_en(\"explosion\", this.x, this.y);\n\n\n      let nave = this.pilas.obtener_actor_por_nombre(\"nave\");\n      nave.decir(\"¬°eso fue f√°cil!\");\n    }\n  }\n}\n"
      },
      {
        "nombre": "texto",
        "codigo": "// @ts-ignore\nclass texto extends ActorTextoBase {\n}\n"
      },
      {
        "nombre": "explosion",
        "codigo": "// @ts-ignore\nclass explosion extends Actor {\n\n  iniciar() {\n    this.animacion = \"explosion\";\n    this.pilas.reproducir_sonido(\"explosion\");\n  }\n\n  actualizar() {}\n\n  cuando_finaliza_animacion(nombre: string) {\n    this.eliminar();\n  }\n}\n"
      },
      {
        "nombre": "reiniciar_escena",
        "codigo": "// @ts-ignore\nclass reiniciar_escena extends ActorTextoBase {\n\n  iniciar() {\n    this.fijo = true;\n  }\n\n  cuando_hace_click() {\n    this.pilas.reiniciar_escena();\n  }\n\n  cuando_mueve() {\n    this.pilas.definir_cursor(\"pointer\");\n  }\n\n  cuando_sale() {\n    this.pilas.definir_cursor(\"normal\");\n  }\n}\n"
      }
